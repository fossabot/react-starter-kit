  "scripts": {
    "start": "vite",
    "build:vite": "vite build",
    "build": "NODE_ENV=production node scripts/build.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "NODE_ENV=test node scripts/test.js",
    "test:debug": "NODE_ENV=test node --inspect-brk scripts/test.js --runInBand --no-cache",
    "refresh": "rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm store prune && pnpm install",
    "check-types": "tsc",
    "check-js": "eslint 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "lint:code": "run-p check-js check-types",
    "lint:style": "prettier 'src/**/*.{scss,sass,css}'",
    "lint": "run-p lint:*",
    "fix:code": "eslint --fix 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "fix:style": "prettier --write 'src/**/*.{scss,sass,css}'",
    "fix": "run-p fix:*"
