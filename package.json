  "dependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "hoist-non-react-statics": "^3.3.2",
    "husky": "^4.3.7",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "path-to-regexp": "^6.2.0",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-test-renderer": "^17.0.1",
    "recoil": "^0.1.2",
    "source-map-explorer": "^2.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "refresh": "rm -rf node_modules && rm -rf yarn.lock && yarn cache clean && yarn",
    "lint:code": "eslint 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "lint:style": "prettier 'src/**/*.{scss,sass,css}'",
    "lint": "run-p lint:*",
    "fix:code": " eslint --fix 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "fix:style": "prettier --write 'src/**/*.{scss,sass,css}'",
    "fix": "run-p fix:*",
    "eject": "react-scripts eject"
  }
}
